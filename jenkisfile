pipeline   {
    agents any 
	stages {
	   stage('COMPILE STAGE'){
	    steps {
		withMaven (maven : 'maven_3_6_3'){
		sh 'mvn clean compile'
		}
		}
		}
		stage('TEST STAGE'){
		   steps {
		   withMaven(maven : 'maven_3_6_3'){
		   sh 'mvn test' 
		   }
		   }
		   }
		 stage ('DEPLOY STAGE'){
           steps {
		   withMaven(maven : 'maven_3_6_3'){
             sh 'mvn deploy'
         }
		 }
		 
	 
	    }
	 }
	}